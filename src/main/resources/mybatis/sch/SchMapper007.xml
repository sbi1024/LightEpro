<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.LightEpro.sch.mapper.SchMapper007">
    <select id="findCreateInfoByCalSeq" resultType="com.example.LightEpro.sch.dto.sch007.SchRqDto007$Calender">
        SELECT TSC.CREATE_SEQ  AS createSeq,
               TSC.CREATE_DATE AS createDate
        FROM T_SC_CAL TSC
        WHERE 1 = 1
          AND TSC.CAL_SEQ = #{calSeq}
          AND TSC.USE_YND = 'Y'
    </select>
</mapper>